{
  "name": "express-lemon",
  "version": "1.0.0",
  "description": "Express based structure and entity generator",
  "main": "server",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha ./test/endToEnd --exit",
    "lint": "./node_modules/.bin/eslint  . *.js",
    "lint:fix": "./node_modules/.bin/eslint  . *.js --fix",
    "start": "./node_modules/pm2/bin/pm2 start ./bin/start_server.json",
    "check:security": "./node_modules/nsp/bin/nsp check",
    "check:modules": "./node_modules/retire/bin/retire",
    "check": "npm run check:security && npm run check:modules && npm run lint",
    "command:create_admin": "node ./commands/user/admin/createAdminUser"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm test",
      "pre-commit": "npm run lint"
    }
  },
  "author": "Roman Sachenko",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "connect-redis": "^3.3.0",
    "dotenv": "^4.0.0",
    "dotenv-safe": "^4.0.4",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "express-validator": "^4.3.0",
    "helmet": "^3.6.1",
    "http-status": "^1.0.1",
    "joi": "^13.0.2",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "mongoose": "^4.10.4",
    "nodemon": "^1.12.5",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google": "^0.3.0",
    "passport-local": "^1.0.0",
    "shelljs": "^0.7.8",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "cool-ascii-faces": "^1.3.4",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "ghooks": "^2.0.2",
    "mocha": "^4.0.1",
    "nsp": "^3.1.0",
    "pm2": "^2.4.6",
    "retire": "^1.4.0",
    "snyk": "^1.65.1",
    "supertest": "^3.0.0"
  }
}
